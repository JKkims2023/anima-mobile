{
  "name": "AnimaMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "dev": "node scripts/dev.js",
    "menu": "node scripts/menu.js",
    "kill": "node scripts/kill-servers.js",
    "setup:icons": "node scripts/setup-icons.js",
    "setup:splash": "node scripts/setup-splash.js",
    "setup:all": "node scripts/setup-icons.js && node scripts/setup-splash.js",
    "postinstall": "cd ios && pod install && cd ..",
    "fix:xcode": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios && xcodebuild clean -workspace AnimaMobile.xcworkspace -scheme AnimaMobile && cd ..",
    "fix:pod": "cd ios && rm -rf Pods Podfile.lock && pod install && cd ..",
    "fix:all": "yarn fix:xcode && yarn fix:pod && yarn start --reset-cache",
    "fix:complete": "bash scripts/fix-all.sh"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@invertase/react-native-apple-authentication": "^2.5.0",
